{
   "manifest_version":2,
   "name":"Autofill Forms Insurance",
   "description":"This extension use to fill insurance form automatically.",
   "version":"0.1",
   "content_scripts":[
      {
         "matches":["<all_urls>"],
         "js":["content.js","stringmanipulation.js","insurance.js"]
      }
   ],
   "background": {
      "scripts": ["background.js","jquery-3.1.0.min.js","stringmanipulation.js","memoryCacher.js"],
      "persistent": true
    },
  "permissions": ["activeTab","storage","tabs"],
  "web_accessible_resources": [
   "data/*.xls","popup.html" ],
  "browser_action":{
      "default_icon":"auto.png",
      "default_title": "fillup form"
   }
}
